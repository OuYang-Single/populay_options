<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_wait"
        tools:context=".mvp.model.mvp.ui.activity.WaitActivity">

    <com.pine.populay_options.mvp.model.mvp.ui.CustomizeView.FullScreenVideoView
            android:id="@+id/wait_ad_video"
               android:layout_width="fill_parent"
               android:layout_height="fill_parent"/>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="@string/wait_jump_over"
                android:textSize="14sp"
                android:id="@+id/wait_time_jump_txt"
                style="@style/style_wait_jump"/>

    </RelativeLayout>
</FrameLayout>